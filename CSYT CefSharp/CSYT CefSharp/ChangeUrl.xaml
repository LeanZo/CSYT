<Window x:Class="CSYT_CefSharp.ChangeUrl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSYT_CefSharp"
        mc:Ignorable="d"
        Title="Change Video" Height="78.896" Width="385.714" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid FocusManager.FocusedElement="{Binding ElementName=TextBox_Url}">
        <TextBox x:Name="TextBox_Url"  HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="NoWrap" Text="" VerticalAlignment="Top" Width="278"/>
        <Button x:Name="Btn_OK" Content="OK" HorizontalAlignment="Left" Margin="293,12,0,0" VerticalAlignment="Top" Width="75" Click="Btn_OK_Click" IsDefault="True"/>
    </Grid>
</Window>
